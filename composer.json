    "type": "project",
    "description": "A simple helper command to clear laravel routes, config, cache, views, compiled, and caches config.",
    "license": "MIT",
    "authors": [
        {
            "name": "Levi Zoesch",
            "email": "levi@traqza.com"
        }
    ],
    "keywords": [
        "Traqza",
        "laravel",
        "laravel commands",
        "laravel helpers",
        "Laravel Clear Everything"
